<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.footballreservationapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/yonamicon"
        android:label="연암공과대 풋살장 예약 앱"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        >
        <activity android:name=".ReservationManageActivity"
            android:screenOrientation="portrait" android:launchMode="singleTop"/>
        <activity android:name=".StudentsManageActivity" android:screenOrientation="portrait" android:launchMode="singleTop"/>
        <activity android:name=".ManageMainActivity" android:screenOrientation="portrait" android:launchMode="singleTop"/>
        <activity android:name=".ReservationCheckPage" android:screenOrientation="portrait" android:launchMode="singleTop"/>
        <activity android:name=".RequestPage" android:screenOrientation="portrait" android:launchMode="singleTop"/>
        <activity android:name=".ReservationPage" android:screenOrientation="portrait" android:launchMode="singleTop"/>
        <activity android:name=".MainActivity" android:screenOrientation="portrait" android:launchMode="singleTop"/>
        <activity android:name=".LoginActivity" android:screenOrientation="portrait" android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"  android:screenOrientation="portrait" android:launchMode="singleTop"/>
    </application>

</manifest>